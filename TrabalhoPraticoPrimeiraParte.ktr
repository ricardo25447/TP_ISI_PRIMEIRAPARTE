      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>logging</jsScript_name>
        <jsScript_script>// ============================================================
// LOG limpo: 1 saída por execução + verificação por moeda
// - executa só na 1.ª linha (evita repetições)
// - "Atualizado" só se preço OU rank mudarem
// ============================================================

// Só corre na primeira linha que chega a este step
if (typeof __already_ran !== 'undefined') {
  output = null;   // não escreve nada nas linhas seguintes
  // termina já (este step corre por linha no PDI)
} else {
  __already_ran = true;

  // --- Ler estado anterior (se existir) ---
  var InputStreamReader = Packages.java.io.InputStreamReader;
  var FileInputStream   = Packages.java.io.FileInputStream;
  var BufferedReader    = Packages.java.io.BufferedReader;
  var File              = Packages.java.io.File;

  var caminhoEstado = "C:\\ETL\\cryptos\\estado_anterior.json";
  var estadoAnterior = {};
  try {
    var f = new File(caminhoEstado);
    if (f.exists()) {
      var br = new BufferedReader(new InputStreamReader(new FileInputStream(f), "UTF-8"));
      var s = "", line;
      while ((line = br.readLine()) != null) s += line;
      br.close();
      estadoAnterior = JSON.parse(s);
    }
  } catch (e) {
    estadoAnterior = {};
  }

  // --- JSON vindo do HTTP Client (campo 'result') pode vir escapado ---
  var clean_json = result.replace(/^"|"$/g, "").replace(/""/g, "\"");

  // --- Parse do JSON para array de moedas ---
  var moedas = [];
  try { moedas = JSON.parse(clean_json); } catch(e){ moedas = []; }

  // --- Data/hora da execução ---
  var now = new Date();
  var data = now.getFullYear()+"-"+("0"+(now.getMonth()+1)).slice(-2)+"-"+("0"+now.getDate()).slice(-2);
  var hora = ("0"+now.getHours()).slice(-2)+":"+("0"+now.getMinutes()).slice(-2)+":"+("0"+now.getSeconds()).slice(-2);

  // --- Montar linhas (uma por moeda) + verificação de alteração (preço/rank) ---
  var linhas = [];
  for (var i=0; i&lt;moedas.length; i++){
    var m = moedas[i];
    var sym   = (m.symbol || "?").toUpperCase();
    var preco = (m.current_price != null) ? m.current_price : "?";
    var rank  = (m.market_cap_rank != null) ? m.market_cap_rank : "?";
    var last  = m.last_updated || "?";

    // estado anterior desta moeda
    var prev = estadoAnterior[sym] || {};
    var precoPrev = (prev.preco != null) ? prev.preco : null;
    var rankPrev  = (prev.rank  != null) ? prev.rank  : null;

    // "Atualizado" apenas se preço OU rank mudarem (ignora last_updated)
    var mudou = (precoPrev === null &amp;&amp; rankPrev === null) || (preco !== precoPrev) || (rank !== rankPrev);
    var status = mudou ? "Atualizado" : "Não atualizado";

    // linha final
    linhas.push(
      "Transformação executada em " + data + " às " + hora +
      " — Moeda: " + sym +
      " — Preço atual: " + preco +
      " — Rank: " + rank +
      " — Última atualização: " + last +
      " — " + status
    );
  }

  // Emitir APENAS UMA saída (com 4 linhas) e um separador no fim
  output = linhas.join("\n") + "\n------------------------------------------------------------";
}
</jsScript_script>
      </jsScript>
    </jsScripts>
